{
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://bitbucket.org/silintl/insite-people-search.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/fillup/Adldap2.git"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "^2.0.9",
    "yiisoft/yii2-swiftmailer": "*",
    "yiisoft/yii2-gii": "*",
    "silinternational/php-env": "^0.1.1",
    "silinternational/yii2-jsonsyslog": "*",
    "silinternational/yii2-email-log-target": "dev-master",
    "silinternational/idp-pw-api-common": "^1.0",
    "silinternational/idp-pw-api-personnel-insite": "^2.0",
    "silinternational/idp-pw-api-auth-saml": "^1.0",
    "silinternational/idp-pw-api-passwordstore-ldap": "^2.0",
    "silinternational/idp-pw-api-passwordstore-id-broker": "^1.1",
    "silinternational/idp-pw-api-phoneverification-nexmo": "^2.0",
    "silinternational/zxcvbn-api-client-php": "^2.0",
    "google/recaptcha": "^1.1.2",
    "adldap2/adldap2": "dev-master",
    "fillup/fake-bower-assets": "2.0.9"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-faker": "*",
    "roave/security-advisories": "dev-master",
    "fillup/phpmyadmin-minimal": "dev-master",
    "codeception/codeception": "^2.2",
    "codeception/specify": "0.*",
    "codeception/verify": "0.*"
  },
  "config": {
    "process-timeout": 1800
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
