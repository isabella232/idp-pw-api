{
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://bitbucket.org/silintl/insite-people-search.git"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "2.0.6",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "yiisoft/yii2-gii": "*",
    "silinternational/php-env": "^0.1.1",
    "silinternational/yii2-jsonsyslog": "*",
    "silinternational/idp-pw-api-common": "dev-master",
    "silinternational/idp-pw-api-personnel-insite": "^0.0.4",
    "silinternational/idp-pw-api-auth-saml": "dev-master",
    "silinternational/idp-pw-api-phoneverification-nexmo": "dev-master",
    "google/recaptcha": "^1.1.2",
    "bjeavons/zxcvbn-php": "^0.1.4",
    "d3th/yii2-zxcvbn-validator": "^v0.1.0"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-faker": "*",
    "roave/security-advisories": "dev-master",
    "fillup/phpmyadmin-minimal": "dev-master",
    "codeception/codeception": "2.0.*",
    "codeception/specify": "0.*",
    "codeception/verify": "0.*"
  },
  "config": {
    "process-timeout": 1800
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
